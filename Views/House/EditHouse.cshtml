@model House

<h2>Edit @Model.LotNum</h2>

@using (Html.BeginForm("EditHouse", "House", "Post"))
{
    <input asp-for="HouseID" type="hidden" value="" />

    <label asp-for="LotNum" class="control-label">Enter a New Lot Number</label>
    <input type="text" name="LotNum" class="form-control" value="" />


    <input type="submit" value="Update Lot Number" class="btn btn-primary" />
}